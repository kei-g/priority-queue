{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "declarationDir": ".",
    "emitDeclarationOnly": true,
    "lib": [
      "ES2015"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "newLine": "lf",
    "pretty": true,
    "removeComments": false,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": false,
    "target": "ES2015",
    "typeRoots": [
      "node_modules/@types",
    ],
  },
  "exclude": [],
  "include": [
    "src",
  ],
}
